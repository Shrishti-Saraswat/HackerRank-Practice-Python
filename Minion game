def minion_game(string):
    Kevin=0
    Stuart=0
    for i in range(len(string)):
        if string[i] in "AEIOU":
            Kevin+=len(string)-i
        else:
            Stuart+=len(string)-i
    if Kevin>Stuart:
        print("Kevin",Kevin)
    elif Stuart>Kevin:
        print("Stuart",Stuart) 
    else:
        print("Draw")                   

    # your code goes here

